{
    "version": 2,
    "builds": [
      {
        "src": "api/predictor.py",
        "use": "@vercel/python",
        "config": {
          "maxDuration": 10,
          "memory": 512
        }
      }
    ],
    "routes": [
      {
        "src": "/predict",
        "dest": "/api/predictor.py"
      }
    ]
  }
  